import java.util.*;
public class IBclass1
{
    void main (String args[])
    {
        Scanner s = new Scanner(System.in);
        {
            double term = 0.2;
            double sum = 0;
            for (int i = 0; i < 100; i++)
            { sum = sum + term;
                term = term / 5;
            }
            System.out.println("total is " + sum);

        } 
    }
}
