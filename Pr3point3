import java.util.*;
public class IBclass1
{
    void main (String args[])
    {
        Scanner s = new Scanner(System.in);
        int n, m, c=0;
        System.out.println("Enter number");
        n = s.nextInt();
        while ( n > 0)
        {
            m = n%10;
            c = c+ (m*m*m);
            n= n/10;
        }
        
        System.out.println("Sum of cube" + c);

    }    
}    
